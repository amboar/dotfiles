[alias]
	co = checkout
	st = status
	ci = commit --signoff
	ca = commit --amend
	rph = rev-parse HEAD
	rvs = remote --verbose show
	f = fetch
	fixes = log -1 --pretty=fixes
	id = log -1 --pretty=id
	ssh = show --stat
	cdiff = diff --cached
	br = branch
	ffm = merge --ff-only
	mtv = mergetool --tool=vimdiff
	rbc = rebase --continue
	cp = cherry-pick
[core]
        abbrev = 12
[pretty]
	id = %h (\"%s\")
	fixes = Fixes: %h (\"%s\")
[user]
	email = andrew@codeconstruct.com.au
	name = Andrew Jeffery
